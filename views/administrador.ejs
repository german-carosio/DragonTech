<!DOCTYPE html>
<html lang="en">

   <!--Head-->
   <%- include ("./partials/head")  %> 

   <body>

   <!--Header-->    
   <%- include ("./partials/header")  %>

   
    <main>
        <section>

            <h2>Creacion y modificacion de productos</h2>

            <article class="crear-producto">

                <h3>Crear</h3>

                <form action="">
                    <input type="text" name="nombre" name="nombre" required placeholder="Nombre">
                    <input type="text" name="precio" name="precio" required placeholder="precio">
                    <input type="text" name="user" name="Nombre" required placeholder="detalle">
                    <input type="text" name="user" name="Nombre" required placeholder="categoria">
                    <button class="bottom-login" type="submit">Crear</button>  
                </form>

            </article>
            
            <article class="modificar-producto">

                <h3>Modificar</h3>

                <form action="">
                    <input type="text" name="nombre" name="nombre" required placeholder="Nombre">
                    <input type="text" name="precio" name="precio" required placeholder="precio">
                    <input type="text" name="user" name="detalle" required placeholder="detalle">
                    <input type="text" name="user" name="categoria" required placeholder="categoria">
                    <button class="bottom-login" type="submit">Modificar</button>   
                </form>

            </article>

        </section>

    </main>
    
    <!--Footer-->
    <%- include ("./partials/footer")  %> 
    
</body>
</html>